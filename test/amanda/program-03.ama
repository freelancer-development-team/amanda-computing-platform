namespace amanda::test;

using std::io::console;
using std::core::math;
using std::core::file;

void do_maths(int x, int y)
{
	if (x > y)
	{
		while (y > 25)
		{
			math::decrementByAmount(y, 5);
		}
	}
	else
	{
		console::println(x);
	}
}

int main(string[] args)
{
	do_maths(pow(2, 31), math::pow(6, 10));
}
